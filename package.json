{
  "name": "nava-language-support",
  "displayName": "Nava Language Support",
  "description": "Nava Langauge Syntax Highlighting Support",
  "icon": "NavaVSCIcon.png",
  "publisher": "Nadelio",
  "author": {
    "name": "Nadelio"
  },
  "version": "1.0.6",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Nadelio/Nava-Language-Support.git"
  },
  "contributes": {
    "languages": [{
      "id": "nava",
      "aliases": ["Nava", "nava"],
      "extensions": [".nava"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/nava-icon.svg",
        "dark": "./icons/nava-icon.svg"
      }
    }],
    "grammars": [{
      "language": "nava",
      "scopeName": "source.nava",
      "path": "./syntaxes/nava.tmLanguage.json"
    }]
  }
}
